{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "app",
    "components",
    "constants",
    "context",
    "hooks",
    "scripts",
    "utils",
    "types.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
